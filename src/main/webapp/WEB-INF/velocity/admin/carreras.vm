#parse( "declaraciones.vm" )
<html>
<head>
	#parse( "head.vm" )
	<script>
		$(document).ready(function(){
			$("#carreras").addClass("active");
		});
	</script>
</head>

<body>
	<div class="container">

		<div class="master-header">
			#parse( "admin/header.vm" )
		</div>

		<div class="master-content">
			<a href="add.html"><button type="button" class="btn btn-default">Nueva</button></a>
			<br><br>
			#if($carreras.empty)
				<div> No hay carreras registradas</div>
			#else
				<table class="table table-hover">
					<tr>
						<th>Carrera</th>
					</tr>
				#foreach($carrera in $carreras)
					<tr>
						<td>$carrera.nombre</td>
					</tr>
				#end
				</table>
			#end
			


		</div>

		<div class="master-footer">
			#parse( "master-footer.vm" )
		</div>
	</div>

</body>
</html>

