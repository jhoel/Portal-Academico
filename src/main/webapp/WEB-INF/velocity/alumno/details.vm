#parse( "declaraciones.vm" )
<html>
<head>
	#parse( "head.vm" )
</head>

<body>
	<div class="container">

		<div class="master-header">
			#parse( "alumno/header.vm" )
		</div>

		<div class="master-content">
		
			<h1>Datos de $alumno.nombre</h1>
			<h2>$alumno.carrera.nombre</h2>

			<table class="table table-hover">
				<tr>
					<td><b>Id:</b></td>
					<td>$alumno.id<td>
				</tr>
				<tr>
					<td><b>Nombre:</b></td>
					<td>$alumno.nombre<td>
				</tr>
				<tr>
					<td><b>Apellido:</b></td>
					<td>$alumno.apellido<td>
				</tr>
				<tr>
					<td><b>Carrera:</b></td>
					<td>$alumno.carrera.nombre<td>
				</tr>
				<tr>
					<td><b>Nombre de Usuario:</b></td>
					<td>$alumno.usuario<td>
				</tr>
				<tr>
					<td><b>Colegio:</b></td>
					<td>$alumno.colegio<td>
				</tr>
				<tr>
					<td><b>Apoderado:</b></td>
					<td>$alumno.apoderado<td>
				</tr>
				<tr>
					<td><b>Telefono:</b></td>
					<td>$alumno.telefono<td>
				</tr>
				<tr>
					<td><b>Fecha de nacimiento:</b></td>
					<td>$alumno.fecha_n<td>
				</tr>
			</table>
			<br />
			
			<a href="edit.html"><button type="button" class="btn btn-default">Modificar</button></a>

		</div>

		<div class="master-footer">
			#parse( "master-footer.vm" )
		</div>
	</div>

</body>
</html>



